# Park by coolio.
#
# Park is a new serial vault that generates wide open areas with lots of trees
# and bushes.
default-depth: D

NAME:   serial_park
TAGS:   allow_dup extra luniq_serial decor layout_open layout_caves
CHANCE: 1%
{{
  local c = 5 + crawl.random2(3)*crawl.random2(2)
  hook("post_place", function()
    dgn.place_maps{tag="serial_park", count=c}
  end)
}}

